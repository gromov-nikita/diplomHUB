          <component id="1c41f" class="javax.swing.JButton" binding="backButton">
            <constraints/>
            <properties>
              <text value="Выйти"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="e2e03" binding="tableScrollPanePanel" layout-manager="FormLayout">
        <rowspec value="center:d:noGrow"/>
        <colspec value="fill:d:grow"/>
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="3" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms top="0" left="5" bottom="0" right="5"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <scrollpane id="68911" binding="tableScrollPane">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
              <forms top="5" left="0" bottom="5" right="5" defaultalign-horz="false" defaultalign-vert="false"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="bb651" class="javax.swing.JTable" binding="requestTable">
                <constraints/>
                <properties>
                  <preferredScrollableViewportSize width="450" height="400"/>
                  <updateSelectionOnSort value="true"/>
                </properties>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
      <grid id="a6bb9" binding="requestLabelPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
