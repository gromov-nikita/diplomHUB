<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.gromov.ui.CustomerProfileForm">
  <grid id="27dc6" binding="customerProfilePanel" layout-manager="FormLayout">
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:4dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <rowspec value="top:4dlu:noGrow"/>
    <rowspec value="center:max(d;4px):noGrow"/>
    <colspec value="fill:d:noGrow"/>
    <colspec value="left:4dlu:noGrow"/>
    <colspec value="fill:d:grow"/>
    <constraints>
      <xy x="20" y="20" width="500" height="172"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="48f67" binding="requestButtonLabel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
        <constraints>
          <grid row="4" column="2" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
          <forms top="0" left="0" bottom="0" right="5"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="43af8" class="javax.swing.JButton" binding="updateTableButton">
            <constraints/>
            <properties>
              <text value="Обновить таблицу"/>
            </properties>
          </component>
          <component id="bf235" class="javax.swing.JButton" binding="commentButton">
            <constraints/>
            <properties>
              <text value="Оставить отзыв"/>
            </properties>
          </component>
          <component id="ea5ee" class="javax.swing.JButton" binding="makeRequestButton">
            <constraints/>
            <properties>
              <text value="Сделать запрос"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="88a2" binding="backButtonPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
